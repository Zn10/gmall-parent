<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper SYSTEM "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.zn.gmall.product.mapper.SpuSaleAttrValueMapper">
    <insert id="batchInsert" parameterType="java.util.List" >
        insert into spu_sale_attr_value (spu_id,base_sale_attr_id,sale_attr_value_name,sale_attr_name) values
        <foreach collection="list" item="item" separator=",">
            (#{item.spuId},#{item.baseSaleAttrId},${saleAttrValueName},${saleAttrName})
        </foreach>
    </insert>

</mapper>
